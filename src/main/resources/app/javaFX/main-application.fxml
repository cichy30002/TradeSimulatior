                </Tab>
                  <Tab text="Test">
                      <content>
                          <VBox>
                           <children>
                              <ListView prefHeight="200.0" prefWidth="200.0" />
                              <TableView prefHeight="200.0" prefWidth="200.0">
                                <columns>
                                  <TableColumn prefWidth="75.0" text="C1" />
                                  <TableColumn prefWidth="75.0" text="C2" />
                                </columns>
                              </TableView>
                           </children></VBox>
                      </content>
                  </Tab>
              </tabs>
            </TabPane>
         </center>
         <right>
            <TabPane prefHeight="800.0" prefWidth="302.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
