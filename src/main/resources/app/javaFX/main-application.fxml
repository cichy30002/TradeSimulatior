                </Tab>
                  <Tab text="Investment Founds">
                      <content>
                          <ListView fx:id="InvestmentFoundList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
                      </content>
                  </Tab>
              </tabs>
            </TabPane>
         </right>
         <left>
            <ListView fx:id="MarketList" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </left>
      </BorderPane>
      <HBox alignment="CENTER" prefHeight="280.0" prefWidth="200.0" spacing="30.0">
